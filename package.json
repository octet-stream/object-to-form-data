    "ci": "yarn coverage && node_modules/.bin/nyc report --reporter=lcov yarn test && node_modules/.bin/codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "2.1.0",
    "ava": "0.25.0",
    "codecov": "3.0.0",
