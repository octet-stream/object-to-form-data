    "eslint-plugin-ava": "4.5.0",
    "formdata-node": "0.3.3",
    "husky": "0.14.3",
    "nyc": "11.4.1",
    "proxyquire": "1.8.0"
  },
  "peerDependencies": {
    "formdata-node": "0.3.3"
  },
  "dependencies": {
    "@octetstream/object-to-form-data": "^0.3.1",
    "lodash.isplainobject": "4.0.6"
